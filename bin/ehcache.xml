<?xml version="1.0" encoding="UTF-8"?>
<!-- 在src目录下，创建一个ehcache.xml用于EHCache的缓存配置 -->
<ehcache>
	<diskStore path="java.io.tmpdir"/>
	<defaultCache 
		maxElementsInMemory="10000"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="true"
	/>
</ehcache>